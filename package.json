{
  "name": "react-boilerplate",
  "version": "1.0.0",
  "description": "Boilerplate setup with webpack and babel config for a React project.",
  "private": true,
  "scripts": {
    "start": "webpack serve --mode=development",
    "build": "webpack --mode=production",
    "format": "prettier --check \"**/*.{js,jsx,ts,tsx,json,md,yml,html}\"",
    "format:fix": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md,yml,html}\""
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jongwooo/react-boilerplate.git"
  },
  "author": "jongwooo <han980817@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jongwooo/react-boilerplate/issues"
  },
  "homepage": "https://github.com/jongwooo/react-boilerplate#readme",
  "packageManager": "yarn@3.2.1",
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.18.9",
    "@babel/preset-env": "^7.18.9",
    "@babel/preset-react": "^7.18.6",
    "@types/react": "^18.0.15",
    "@types/react-dom": "^18.0.6",
    "babel-loader": "^8.2.5",
    "clean-webpack-plugin": "^4.0.0",
    "compression-webpack-plugin": "^10.0.0",
    "css-loader": "^6.7.1",
    "html-webpack-plugin": "^5.5.0",
    "mini-css-extract-plugin": "^2.6.1",
    "prettier": "^2.7.1",
    "react-hot-loader": "^4.13.0",
    "sass": "^1.53.0",
    "sass-loader": "^13.0.2",
    "style-loader": "^3.3.1",
    "ts-loader": "^9.3.1",
    "typescript": "^4.7.4",
    "webpack": "^5.73.0",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^4.9.3"
  }
}
